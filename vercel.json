{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/sessions.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sessions(.*)",
      "dest": "/api/sessions.js"
    }
  ]
} 